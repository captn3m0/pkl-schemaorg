module org.schema.experimental
import "package://pkg.pkl-lang.org/pkl-pantry/pkl.experimental.uri@1.0.3#/URI.pkl" as URL
import "CreativeWork.pkl"
/// A work of art that is primarily visual in character.
///
/// See: <https://schema.org/VisualArtwork>
/// Model depth: 3
open class VisualArtwork extends CreativeWork.CreativeWork {
        
    /// e.g. Painting, Drawing, Sculpture, Print, Photograph, Assemblage, Collage, etc.
    artform: (Listing<URL|String>|URL|String)?
        
    /// The material used. (E.g. Oil, Watercolour, Acrylic, Linoprint, Marble, Cyanotype,
    /// Digital, Lithograph, DryPoint, Intaglio, Pastel, Woodcut, Pencil, Mixed Media, etc.)
    artMedium: (Listing<URL|String>|URL|String)?
        
    /// The individual who adds lettering, including speech balloons and sound effects, to
    /// artwork.
    letterer: (Listing<String>|String)?
        
    /// The depth of the item.
    depth: (Listing<String>|String)?
        
    /// The individual who traces over the pencil drawings in ink after pencils are complete.
    inker: (Listing<String>|String)?
        
    /// A material used as a surface in some artwork, e.g. Canvas, Paper, Wood, Board, etc.
    surface: (Listing<URL|String>|URL|String)?
        
    /// The height of the item.
    height: (Listing<String>|String)?
        
    /// The supporting materials for the artwork, e.g. Canvas, Paper, Wood, Board, etc.
    artworkSurface: (Listing<URL|String>|URL|String)?
        
    /// The primary artist for a work in a medium other than pencils or digital line art--for example,
    /// if the primary artwork is done in watercolors or digital paints.
    artist: (Listing<String>|String)?
        
    /// The width of the item.
    width: (Listing<String>|String)?
        
    /// The individual who adds color to inked drawings.
    colorist: (Listing<String>|String)?
        
    /// The individual who draws the primary narrative artwork.
    penciler: (Listing<String>|String)?
        
    /// The number of copies when multiple copies of a piece of artwork are produced - e.g. for
    /// a limited edition of 20 prints, 'artEdition' refers to the total number of copies (in
    /// this example "20").
    artEdition: (Listing<Int|String>|Int|String)?
    
}