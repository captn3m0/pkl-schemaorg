module org.schema.experimental
import "package://pkg.pkl-lang.org/pkl-pantry/pkl.experimental.uri@1.0.3#/URI.pkl" as URL
import "CreativeWork.pkl"
/// A musical composition.
///
/// See: <https://schema.org/MusicComposition>
/// Model depth: 3
open class MusicComposition extends CreativeWork.CreativeWork {
        
    /// The date and place the work was first performed.
    firstPerformance: (Listing<String>|String)?
        
    /// Smaller compositions included in this work (e.g. a movement in a symphony).
    includedComposition: (Listing<MusicComposition|String>|MusicComposition|String)?
        
    /// The person or organization who wrote a composition, or who is the composer of a work performed
    /// at some event.
    composer: (Listing<String>|String)?
        
    /// An arrangement derived from the composition.
    musicArrangement: (Listing<MusicComposition|String>|MusicComposition|String)?
        
    /// The International Standard Musical Work Code for the composition.
    iswcCode: (Listing<String>|String)?
        
    /// The key, mode, or scale this composition uses.
    musicalKey: (Listing<String>|String)?
        
    /// The person who wrote the words.
    lyricist: (Listing<String>|String)?
        
    /// An audio recording of the work.
    recordedAs: (Listing<String>|String)?
        
    /// The type of composition (e.g. overture, sonata, symphony, etc.).
    musicCompositionForm: (Listing<String>|String)?
        
    /// The words in the song.
    lyrics: (Listing<String>|String)?
    
}