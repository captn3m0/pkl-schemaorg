module org.schema.experimental
import "package://pkg.pkl-lang.org/pkl-pantry/pkl.experimental.uri@1.0.3#/URI.pkl" as URL
import "BioChemEntity.pkl"
/// Any constitutionally or isotopically distinct atom, molecule, ion, ion pair, radical,
/// radical ion, complex, conformer etc., identifiable as a separately distinguishable
/// entity.
///
/// See: <https://schema.org/MolecularEntity>
/// Model depth: 3
open class MolecularEntity extends BioChemEntity.BioChemEntity {
        
    /// The empirical formula is the simplest whole number ratio of all the atoms in a molecule.
    molecularFormula: (Listing<String>|String)?
        
    /// Systematic method of naming chemical compounds as recommended by the International
    /// Union of Pure and Applied Chemistry (IUPAC).
    iupacName: (Listing<String>|String)?
        
    /// A role played by the BioChemEntity within a chemical context.
    chemicalRole: (Listing<String>|String)?
        
    /// Intended use of the BioChemEntity by humans.
    potentialUse: (Listing<String>|String)?
        
    /// A specification in form of a line notation for describing the structure of chemical species
    /// using short ASCII strings. Double bond stereochemistry \ indicators may need to be escaped
    /// in the string in formats where the backslash is an escape character.
    smiles: (Listing<String>|String)?
        
    /// InChIKey is a hashed version of the full InChI (using the SHA-256 algorithm).
    inChIKey: (Listing<String>|String)?
        
    /// Non-proprietary identifier for molecular entity that can be used in printed and electronic
    /// data sources thus enabling easier linking of diverse data compilations.
    inChI: (Listing<String>|String)?
        
    /// The monoisotopic mass is the sum of the masses of the atoms in a molecule using the unbound,
    /// ground-state, rest mass of the principal (most abundant) isotope for each element instead
    /// of the isotopic average mass. Please include the units in the form '&lt;Number&gt; &lt;unit&gt;',
    /// for example '770.230488 g/mol' or as '&lt;QuantitativeValue&gt;.
    monoisotopicMolecularWeight: (Listing<String>|String)?
        
    /// This is the molecular weight of the entity being described, not of the parent. Units should
    /// be included in the form '&lt;Number&gt; &lt;unit&gt;', for example '12 amu' or as '&lt;QuantitativeValue&gt;.
    molecularWeight: (Listing<String>|String)?
    
}