module org.schema.experimental
import "package://pkg.pkl-lang.org/pkl-pantry/pkl.experimental.uri@1.0.3#/URI.pkl" as URL
import "StructuredValue.pkl"
/// A structured value indicating the quantity, unit of measurement, and business function
/// of goods included in a bundle offer.
///
/// See: <https://schema.org/TypeAndQuantityNode>
/// Model depth: 4
open class TypeAndQuantityNode extends StructuredValue.StructuredValue {
        
    /// The business function (e.g. sell, lease, repair, dispose) of the offer or component
    /// of a bundle (TypeAndQuantityNode). The default is http://purl.org/goodrelations/v1#Sell.
    businessFunction: (Listing<String>|String)?
        
    /// The product that this structured value is referring to.
    typeOfGood: (Listing<String>|String)?
        
    /// The unit of measurement given using the UN/CEFACT Common Code (3 characters) or a URL.
    /// Other codes than the UN/CEFACT Common Code may be used with a prefix followed by a colon.
    unitCode: (Listing<URL|String>|URL|String)?
        
    /// A string or text indicating the unit of measurement. Useful if you cannot provide a standard
    /// unit code for <a href='unitCode'>unitCode</a>.
    unitText: (Listing<String>|String)?
        
    /// The quantity of the goods included in the offer.
    amountOfThisGood: (Listing<Int|Float|String>|Int|Float|String)?
    
}