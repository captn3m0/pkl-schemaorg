module org.schema.experimental
import "package://pkg.pkl-lang.org/pkl-pantry/pkl.experimental.uri@1.0.3#/URI.pkl" as URL
import "MedicalAudience.pkl"
import "Person.pkl"
/// A patient is any person recipient of health care services.
///
/// See: <https://schema.org/Patient>
/// Model depth: 3
open class Patient extends MedicalAudience.MedicalAudience Person.Person {
        
    /// Specifying a drug or medicine used in a medication procedure.
    drug: (Listing<String>|String)?
        
    /// Specifying the health condition(s) of a patient, medical study, or other target audience.
    healthCondition: (Listing<String>|String)?
        
    /// One or more alternative conditions considered in the differential diagnosis process
    /// as output of a diagnosis process.
    diagnosis: (Listing<String>|String)?
    
}