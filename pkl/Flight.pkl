module org.schema.experimental
import "package://pkg.pkl-lang.org/pkl-pantry/pkl.experimental.uri@1.0.3#/URI.pkl" as URL
import "Trip.pkl"
/// An airline flight.
///
/// See: <https://schema.org/Flight>
/// Model depth: 4
open class Flight extends Trip.Trip {
        
    /// The type of boarding policy used by the airline (e.g. zone-based or group-based).
    boardingPolicy: (Listing<String>|String)?
        
    /// The estimated time the flight will take.
    estimatedFlightDuration: (Listing<String>|String)?
        
    /// 'carrier' is an out-dated term indicating the 'provider' for parcel delivery and flights.
    carrier: (Listing<String>|String)?
        
    /// An entity which offers (sells / leases / lends / loans) the services / goods. A seller may
    /// also be a provider.
    seller: (Listing<String>|String)?
        
    /// The distance of the flight.
    flightDistance: (Listing<String>|String)?
        
    /// Identifier of the flight's arrival terminal.
    arrivalTerminal: (Listing<String>|String)?
        
    /// Description of the meals that will be provided or available for purchase.
    mealService: (Listing<String>|String)?
        
    /// Identifier of the flight's arrival gate.
    arrivalGate: (Listing<String>|String)?
        
    /// The airport where the flight originates.
    departureAirport: (Listing<String>|String)?
        
    /// The unique identifier for a flight including the airline IATA code. For example, if describing
    /// United flight 110, where the IATA code for United is 'UA', the flightNumber is 'UA110'.
    flightNumber: (Listing<String>|String)?
        
    /// Identifier of the flight's departure gate.
    departureGate: (Listing<String>|String)?
        
    /// The kind of aircraft (e.g., "Boeing 747").
    aircraft: (Listing<String>|String)?
        
    /// Identifier of the flight's departure terminal.
    departureTerminal: (Listing<String>|String)?
        
    /// The airport where the flight terminates.
    arrivalAirport: (Listing<String>|String)?
        
    /// The time when a passenger can check into the flight online.
    webCheckinTime: (Listing<String>|String)?
    
}