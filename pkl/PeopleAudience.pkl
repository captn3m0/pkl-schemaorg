module org.schema.experimental
import "package://pkg.pkl-lang.org/pkl-pantry/pkl.experimental.uri@1.0.3#/URI.pkl" as URL
import "Audience.pkl"
/// A set of characteristics belonging to people, e.g. who compose an item's target audience.
///
/// See: <https://schema.org/PeopleAudience>
/// Model depth: 4
open class PeopleAudience extends Audience.Audience {
        
    /// Minimum recommended age in years for the audience or user.
    suggestedMinAge: (Listing<Int|Float|String>|Int|Float|String)?
        
    /// A suggested range of body measurements for the intended audience or person, for example
    /// inseam between 32 and 34 inches or height between 170 and 190 cm. Typically found on a size
    /// chart for wearable products.
    suggestedMeasurement: (Listing<String>|String)?
        
    /// Audiences defined by a person's maximum age.
    requiredMaxAge: (Listing<Int|String>|Int|String)?
        
    /// Specifying the health condition(s) of a patient, medical study, or other target audience.
    healthCondition: (Listing<String>|String)?
        
    /// The suggested gender of the intended person or audience, for example "male", "female",
    /// or "unisex".
    suggestedGender: (Listing<String>|String)?
        
    /// Audiences defined by a person's gender.
    requiredGender: (Listing<String>|String)?
        
    /// The age or age range for the intended audience or person, for example 3-12 months for infants,
    /// 1-5 years for toddlers.
    suggestedAge: (Listing<String>|String)?
        
    /// Maximum recommended age in years for the audience or user.
    suggestedMaxAge: (Listing<Int|Float|String>|Int|Float|String)?
        
    /// Audiences defined by a person's minimum age.
    requiredMinAge: (Listing<Int|String>|Int|String)?
    
}