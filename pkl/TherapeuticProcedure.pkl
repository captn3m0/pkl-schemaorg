module org.schema.experimental
import "package://pkg.pkl-lang.org/pkl-pantry/pkl.experimental.uri@1.0.3#/URI.pkl" as URL
import "MedicalProcedure.pkl"
/// A medical procedure intended primarily for therapeutic purposes, aimed at improving
/// a health condition.
///
/// See: <https://schema.org/TherapeuticProcedure>
/// Model depth: 4
open class TherapeuticProcedure extends MedicalProcedure.MedicalProcedure {
        
    /// Specifying a drug or medicine used in a medication procedure.
    drug: (Listing<String>|String)?
        
    /// A possible complication and/or side effect of this therapy. If it is known that an adverse
    /// outcome is serious (resulting in death, disability, or permanent damage; requiring
    /// hospitalization; or otherwise life-threatening or requiring immediate medical attention),
    /// tag it as a seriousAdverseOutcome instead.
    adverseOutcome: (Listing<String>|String)?
        
    /// A dosing schedule for the drug for a given population, either observed, recommended,
    /// or maximum dose based on the type used.
    doseSchedule: (Listing<String>|String)?
    
}