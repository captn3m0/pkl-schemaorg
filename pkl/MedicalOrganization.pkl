module org.schema.experimental
import "package://pkg.pkl-lang.org/pkl-pantry/pkl.experimental.uri@1.0.3#/URI.pkl" as URL
import "Organization.pkl"
/// A medical organization (physical or not), such as hospital, institution or clinic.
///
/// See: <https://schema.org/MedicalOrganization>
/// Model depth: 3
open class MedicalOrganization extends Organization.Organization {
        
    /// Name or unique ID of network. (Networks are often reused across different insurance
    /// plans.)
    healthPlanNetworkId: (Listing<String>|String)?
        
    /// Whether the provider is accepting new patients.
    isAcceptingNewPatients: (Listing<Boolean|String>|Boolean|String)?
        
    /// A medical specialty of the provider.
    medicalSpecialty: (Listing<String>|String)?
    
}