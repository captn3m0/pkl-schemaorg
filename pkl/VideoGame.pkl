module org.schema.experimental
import "package://pkg.pkl-lang.org/pkl-pantry/pkl.experimental.uri@1.0.3#/URI.pkl" as URL
import "Game.pkl"
import "SoftwareApplication.pkl"
/// A video game is an electronic game that involves human interaction with a user interface
/// to generate visual feedback on a video device.
///
/// See: <https://schema.org/VideoGame>
/// Model depth: 4
open class VideoGame extends Game.Game SoftwareApplication.SoftwareApplication {
        
    /// The server on which it is possible to play the game.
    gameServer: (Listing<String>|String)?
        
    /// An actor, e.g. in TV, radio, movie, video games etc. Actors can be associated with individual
    /// items or with a series, episode, clip.
    actors: (Listing<String>|String)?
        
    /// An actor, e.g. in TV, radio, movie, video games etc., or in an event. Actors can be associated
    /// with individual items or with a series, episode, clip.
    actor: (Listing<String>|String)?
        
    /// A director of e.g. TV, radio, movie, video gaming etc. content, or of an event. Directors
    /// can be associated with individual items or with a series, episode, clip.
    director: (Listing<String>|String)?
        
    /// The trailer of a movie or TV/radio series, season, episode, etc.
    trailer: (Listing<String>|String)?
        
    /// The composer of the soundtrack.
    musicBy: (Listing<String>|String)?
        
    /// Indicates whether this game is multi-player, co-op or single-player. The game can be
    /// marked as multi-player, co-op and single-player at the same time.
    playMode: (Listing<String>|String)?
        
    /// A director of e.g. TV, radio, movie, video games etc. content. Directors can be associated
    /// with individual items or with a series, episode, clip.
    directors: (Listing<String>|String)?
        
    /// The electronic systems used to play <a href="http://en.wikipedia.org/wiki/Category:Video_game_platforms">video
    /// games</a>.
    gamePlatform: (Listing<URL|String>|URL|String)?
        
    /// The edition of a video game.
    gameEdition: (Listing<String>|String)?
        
    /// Cheat codes to the game.
    cheatCode: (Listing<String>|String)?
        
    /// Links to tips, tactics, etc.
    gameTip: (Listing<String>|String)?
    
}