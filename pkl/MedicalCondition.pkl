module org.schema.experimental
import "package://pkg.pkl-lang.org/pkl-pantry/pkl.experimental.uri@1.0.3#/URI.pkl" as URL
import "MedicalEntity.pkl"
/// Any condition of the human body that affects the normal functioning of a person, whether
/// physically or mentally. Includes diseases, injuries, disabilities, disorders, syndromes,
/// etc.
///
/// See: <https://schema.org/MedicalCondition>
/// Model depth: 3
open class MedicalCondition extends MedicalEntity.MedicalEntity {
        
    /// Specifying a drug or medicine used in a medication procedure.
    drug: (Listing<String>|String)?
        
    /// A sign or symptom of this condition. Signs are objective or physically observable manifestations
    /// of the medical condition while symptoms are the subjective experience of the medical
    /// condition.
    signOrSymptom: (Listing<String>|String)?
        
    /// The status of the study (enumerated).
    status: (Listing<String>|String)?
        
    /// The stage of the condition, if applicable.
    stage: (Listing<String>|String)?
        
    /// Changes in the normal mechanical, physical, and biochemical functions that are associated
    /// with this activity or condition.
    pathophysiology: (Listing<String>|String)?
        
    /// A preventative therapy used to prevent reoccurrence of the medical condition after
    /// an initial episode of the condition.
    secondaryPrevention: (Listing<String>|String)?
        
    /// The expected progression of the condition if it is not treated and allowed to progress
    /// naturally.
    naturalProgression: (Listing<String>|String)?
        
    /// A modifiable or non-modifiable factor that increases the risk of a patient contracting
    /// this condition, e.g. age, coexisting condition.
    riskFactor: (Listing<String>|String)?
        
    /// A medical test typically performed given this condition.
    typicalTest: (Listing<String>|String)?
        
    /// The likely outcome in either the short term or long term of the medical condition.
    expectedPrognosis: (Listing<String>|String)?
        
    /// The anatomy of the underlying organ system or structures associated with this entity.
    associatedAnatomy: (Listing<String>|String)?
        
    /// A possible treatment to address this condition, sign or symptom.
    possibleTreatment: (Listing<String>|String)?
        
    /// One of a set of differential diagnoses for the condition. Specifically, a closely-related
    /// or competing diagnosis typically considered later in the cognitive process whereby
    /// this medical condition is distinguished from others most likely responsible for a similar
    /// collection of signs and symptoms to reach the most parsimonious diagnosis or diagnoses
    /// in a patient.
    differentialDiagnosis: (Listing<String>|String)?
        
    /// A possible unexpected and unfavorable evolution of a medical condition. Complications
    /// may include worsening of the signs or symptoms of the disease, extension of the condition
    /// to other organ systems, etc.
    possibleComplication: (Listing<String>|String)?
        
    /// A preventative therapy used to prevent an initial occurrence of the medical condition,
    /// such as vaccination.
    primaryPrevention: (Listing<String>|String)?
        
    /// The characteristics of associated patients, such as age, gender, race etc.
    epidemiology: (Listing<String>|String)?
    
}