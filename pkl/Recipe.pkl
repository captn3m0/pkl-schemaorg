module org.schema.experimental
import "package://pkg.pkl-lang.org/pkl-pantry/pkl.experimental.uri@1.0.3#/URI.pkl" as URL
import "HowTo.pkl"
/// A recipe. For dietary restrictions covered by the recipe, a few common restrictions
/// are enumerated via [[suitableForDiet]]. The [[keywords]] property can also be used
/// to add more detail.
///
/// See: <https://schema.org/Recipe>
/// Model depth: 4
open class Recipe extends HowTo.HowTo {
        
    /// A step in making the recipe, in the form of a single item (document, video, etc.) or an ordered
    /// list with HowToStep and/or HowToSection items.
    recipeInstructions: (Listing<String>|String)?
        
    /// The method of cooking, such as Frying, Steaming, ...
    cookingMethod: (Listing<String>|String)?
        
    /// A single ingredient used in the recipe, e.g. sugar, flour or garlic.
    ingredients: (Listing<String>|String)?
        
    /// The quantity produced by the recipe (for example, number of people served, number of
    /// servings, etc).
    recipeYield: (Listing<String>|String)?
        
    /// The category of the recipeâ€”for example, appetizer, entree, etc.
    recipeCategory: (Listing<String>|String)?
        
    /// The time it takes to actually cook the dish, in [ISO 8601 duration format](http://en.wikipedia.org/wiki/ISO_8601).
    cookTime: (Listing<String>|String)?
        
    /// A single ingredient used in the recipe, e.g. sugar, flour or garlic.
    recipeIngredient: (Listing<String>|String)?
        
    /// Nutrition information about the recipe or menu item.
    nutrition: (Listing<String>|String)?
        
    /// Indicates a dietary restriction or guideline for which this recipe or menu item is suitable,
    /// e.g. diabetic, halal etc.
    suitableForDiet: (Listing<String>|String)?
        
    /// The cuisine of the recipe (for example, French or Ethiopian).
    recipeCuisine: (Listing<String>|String)?
    
}