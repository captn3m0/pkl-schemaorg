module org.schema.experimental
import "package://pkg.pkl-lang.org/pkl-pantry/pkl.experimental.uri@1.0.3#/URI.pkl" as URL
import "Substance.pkl"
import "Product.pkl"
/// A chemical or biologic substance, used as a medical therapy, that has a physiological
/// effect on an organism. Here the term drug is used interchangeably with the term medicine
/// although clinical knowledge makes a clear difference between them.
///
/// See: <https://schema.org/Drug>
/// Model depth: 3
open class Drug extends Substance.Substance Product.Product {
        
    /// The specific biochemical interaction through which this drug or supplement produces
    /// its pharmacological effect.
    mechanismOfAction: (Listing<String>|String)?
        
    /// Any other drug related to this one, for example commonly-prescribed alternatives.
    relatedDrug: (Listing<Drug|String>|Drug|String)?
        
    /// A route by which this drug may be administered, e.g. 'oral'.
    administrationRoute: (Listing<String>|String)?
        
    /// The drug or supplement's legal status, including any controlled substance schedules
    /// that apply.
    legalStatus: (Listing<String>|String)?
        
    /// True if this item's name is a proprietary/brand name (vs. generic name).
    isProprietary: (Listing<Boolean|String>|Boolean|String)?
        
    /// The unit in which the drug is measured, e.g. '5 mg tablet'.
    drugUnit: (Listing<String>|String)?
        
    /// Recommended intake of this supplement for a given population as defined by a specific
    /// recommending authority.
    maximumIntake: (Listing<String>|String)?
        
    /// Pregnancy category of this drug.
    pregnancyCategory: (Listing<String>|String)?
        
    /// Any precaution, guidance, contraindication, etc. related to consumption of alcohol
    /// while taking this drug.
    alcoholWarning: (Listing<String>|String)?
        
    /// Any information related to overdose on a drug, including signs or symptoms, treatments,
    /// contact information for emergency response.
    overdosage: (Listing<String>|String)?
        
    /// True if the drug is available in a generic form (regardless of name).
    isAvailableGenerically: (Listing<Boolean|String>|Boolean|String)?
        
    /// Link to prescribing information for the drug.
    prescribingInfo: (Listing<URL|String>|URL|String)?
        
    /// The generic name of this drug or supplement.
    nonProprietaryName: (Listing<String>|String)?
        
    /// A dosage form in which this drug/supplement is available, e.g. 'tablet', 'suspension',
    /// 'injection'.
    dosageForm: (Listing<String>|String)?
        
    /// The insurance plans that cover this drug.
    includedInHealthInsurancePlan: (Listing<String>|String)?
        
    /// The class of drug this belongs to (e.g., statins).
    drugClass: (Listing<String>|String)?
        
    /// Link to the drug's label details.
    labelDetails: (Listing<URL|String>|URL|String)?
        
    /// Indicates the status of drug prescription, e.g. local catalogs classifications or
    /// whether the drug is available by prescription or over-the-counter, etc.
    prescriptionStatus: (Listing<String>|String)?
        
    /// A dosing schedule for the drug for a given population, either observed, recommended,
    /// or maximum dose based on the type used.
    doseSchedule: (Listing<String>|String)?
        
    /// An active ingredient, typically chemical compounds and/or biologic substances.
    activeIngredient: (Listing<String>|String)?
        
    /// Description of the absorption and elimination of drugs, including their concentration
    /// (pharmacokinetics, pK) and biological effects (pharmacodynamics, pD).
    clinicalPharmacology: (Listing<String>|String)?
        
    /// Any precaution, guidance, contraindication, etc. related to this drug's use during
    /// pregnancy.
    pregnancyWarning: (Listing<String>|String)?
        
    /// Proprietary name given to the diet plan, typically by its originator or creator.
    proprietaryName: (Listing<String>|String)?
        
    /// Another drug that is known to interact with this drug in a way that impacts the effect of
    /// this drug or causes a risk to the patient. Note: disease interactions are typically captured
    /// as contraindications.
    interactingDrug: (Listing<Drug|String>|Drug|String)?
        
    /// Any precaution, guidance, contraindication, etc. related to this drug's use by breastfeeding
    /// mothers.
    breastfeedingWarning: (Listing<String>|String)?
        
    /// Any precaution, guidance, contraindication, etc. related to consumption of specific
    /// foods while taking this drug.
    foodWarning: (Listing<String>|String)?
        
    /// The RxCUI drug identifier from RXNORM.
    rxcui: (Listing<String>|String)?
        
    /// An available dosage strength for the drug.
    availableStrength: (Listing<String>|String)?
        
    /// Any FDA or other warnings about the drug (text or URL).
    warning: (Listing<URL|String>|URL|String)?
        
    /// Description of the absorption and elimination of drugs, including their concentration
    /// (pharmacokinetics, pK) and biological effects (pharmacodynamics, pD).
    clincalPharmacology: (Listing<String>|String)?
    
}