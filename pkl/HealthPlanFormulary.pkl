module org.schema.experimental
import "package://pkg.pkl-lang.org/pkl-pantry/pkl.experimental.uri@1.0.3#/URI.pkl" as URL
import "Intangible.pkl"
/// For a given health insurance plan, the specification for costs and coverage of prescription
/// drugs.
///
/// See: <https://schema.org/HealthPlanFormulary>
/// Model depth: 3
open class HealthPlanFormulary extends Intangible.Intangible {
        
    /// The tier(s) of drugs offered by this formulary or insurance plan.
    healthPlanDrugTier: (Listing<String>|String)?
        
    /// Whether prescriptions can be delivered by mail.
    offersPrescriptionByMail: (Listing<Boolean|String>|Boolean|String)?
        
    /// The costs to the patient for services under this network or formulary.
    healthPlanCostSharing: (Listing<Boolean|String>|Boolean|String)?
    
}