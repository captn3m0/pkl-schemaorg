module org.schema.experimental
import "package://pkg.pkl-lang.org/pkl-pantry/pkl.experimental.uri@1.0.3#/URI.pkl" as URL
import "LifestyleModification.pkl"
import "CreativeWork.pkl"
/// A strategy of regulating the intake of food to achieve or maintain a specific health-related
/// goal.
///
/// See: <https://schema.org/Diet>
/// Model depth: 3
open class Diet extends LifestyleModification.LifestyleModification CreativeWork.CreativeWork {
        
    /// Specific physiologic risks associated to the diet plan.
    risks: (Listing<String>|String)?
        
    /// Medical expert advice related to the plan.
    expertConsiderations: (Listing<String>|String)?
        
    /// People or organizations that endorse the plan.
    endorsers: (Listing<String>|String)?
        
    /// Nutritional information specific to the dietary plan. May include dietary recommendations
    /// on what foods to avoid, what foods to consume, and specific alterations/deviations
    /// from the USDA or other regulatory body's approved dietary guidelines.
    dietFeatures: (Listing<String>|String)?
        
    /// Specific physiologic benefits associated to the plan.
    physiologicalBenefits: (Listing<String>|String)?
    
}