module org.schema.experimental
import "package://pkg.pkl-lang.org/pkl-pantry/pkl.experimental.uri@1.0.3#/URI.pkl" as URL
import "Intangible.pkl"
/// An entry point, within some Web-based protocol.
///
/// See: <https://schema.org/EntryPoint>
/// Model depth: 3
open class EntryPoint extends Intangible.Intangible {
        
    /// The high level platform(s) where the Action can be performed for the given URL. To specify
    /// a specific application or operating system instance, use actionApplication.
    actionPlatform: (Listing<URL|String>|URL|String)?
        
    /// An application that can complete the request.
    actionApplication: (Listing<String>|String)?
        
    /// An application that can complete the request.
    application: (Listing<String>|String)?
        
    /// An HTTP method that specifies the appropriate HTTP method for a request to an HTTP EntryPoint.
    /// Values are capitalized strings as used in HTTP.
    httpMethod: (Listing<String>|String)?
        
    /// An url template (RFC6570) that will be used to construct the target of the execution of
    /// the action.
    urlTemplate: (Listing<String>|String)?
        
    /// The supported content type(s) for an EntryPoint response.
    contentType: (Listing<String>|String)?
        
    /// The supported encoding type(s) for an EntryPoint request.
    encodingType: (Listing<String>|String)?
    
}