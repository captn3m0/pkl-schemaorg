module org.schema.experimental
import "package://pkg.pkl-lang.org/pkl-pantry/pkl.experimental.uri@1.0.3#/URI.pkl" as URL
import "StructuredValue.pkl"
/// A CDCPMDRecord is a data structure representing a record in a CDC tabular data format
/// used for hospital data reporting. See [documentation](/docs/cdc-covid.html) for
/// details, and the linked CDC materials for authoritative definitions used as the source
/// here.
///
/// See: <https://schema.org/CDCPMDRecord>
/// Model depth: 4
open class CDCPMDRecord extends StructuredValue.StructuredValue {
        
    /// numc19overflowpats - ED/OVERFLOW: Patients with suspected or confirmed COVID-19
    /// who are in the ED or any overflow location awaiting an inpatient bed.
    cvdNumC19OverflowPats: (Listing<Int|Float|String>|Int|Float|String)?
        
    /// numicubeds - ICU BEDS: Total number of staffed inpatient intensive care unit (ICU) beds.
    cvdNumICUBeds: (Listing<Int|Float|String>|Int|Float|String)?
        
    /// numc19mechventpats - HOSPITALIZED and VENTILATED: Patients hospitalized in an NHSN
    /// inpatient care location who have suspected or confirmed COVID-19 and are on a mechanical
    /// ventilator.
    cvdNumC19MechVentPats: (Listing<Int|Float|String>|Int|Float|String)?
        
    /// collectiondate - Date for which patient counts are reported.
    cvdCollectionDate: (Listing<String|String>|String|String)?
        
    /// numicubedsocc - ICU BED OCCUPANCY: Total number of staffed inpatient ICU beds that are
    /// occupied.
    cvdNumICUBedsOcc: (Listing<Int|Float|String>|Int|Float|String)?
        
    /// Publication date of an online listing.
    datePosted: (Listing<String|String>|String|String)?
        
    /// numc19hopats - HOSPITAL ONSET: Patients hospitalized in an NHSN inpatient care location
    /// with onset of suspected or confirmed COVID-19 14 or more days after hospitalization.
    cvdNumC19HOPats: (Listing<Int|Float|String>|Int|Float|String)?
        
    /// Identifier of the NHSN facility that this data record applies to. Use [[cvdFacilityCounty]]
    /// to indicate the county. To provide other details, [[healthcareReportingData]] can
    /// be used on a [[Hospital]] entry.
    cvdFacilityId: (Listing<String>|String)?
        
    /// numventuse - MECHANICAL VENTILATORS IN USE: Total number of ventilators in use.
    cvdNumVentUse: (Listing<Int|Float|String>|Int|Float|String)?
        
    /// numc19hosppats - HOSPITALIZED: Patients currently hospitalized in an inpatient care
    /// location who have suspected or confirmed COVID-19.
    cvdNumC19HospPats: (Listing<Int|Float|String>|Int|Float|String)?
        
    /// numbeds - HOSPITAL INPATIENT BEDS: Inpatient beds, including all staffed, licensed,
    /// and overflow (surge) beds used for inpatients.
    cvdNumBeds: (Listing<Int|Float|String>|Int|Float|String)?
        
    /// Name of the County of the NHSN facility that this data record applies to. Use [[cvdFacilityId]]
    /// to identify the facility. To provide other details, [[healthcareReportingData]]
    /// can be used on a [[Hospital]] entry.
    cvdFacilityCounty: (Listing<String>|String)?
        
    /// numvent - MECHANICAL VENTILATORS: Total number of ventilators available.
    cvdNumVent: (Listing<Int|Float|String>|Int|Float|String)?
        
    /// numc19ofmechventpats - ED/OVERFLOW and VENTILATED: Patients with suspected or confirmed
    /// COVID-19 who are in the ED or any overflow location awaiting an inpatient bed and on a mechanical
    /// ventilator.
    cvdNumC19OFMechVentPats: (Listing<Int|Float|String>|Int|Float|String)?
        
    /// numc19died - DEATHS: Patients with suspected or confirmed COVID-19 who died in the hospital,
    /// ED, or any overflow location.
    cvdNumC19Died: (Listing<Int|Float|String>|Int|Float|String)?
        
    /// numtotbeds - ALL HOSPITAL BEDS: Total number of all inpatient and outpatient beds, including
    /// all staffed, ICU, licensed, and overflow (surge) beds used for inpatients or outpatients.
    cvdNumTotBeds: (Listing<Int|Float|String>|Int|Float|String)?
        
    /// numbedsocc - HOSPITAL INPATIENT BED OCCUPANCY: Total number of staffed inpatient beds
    /// that are occupied.
    cvdNumBedsOcc: (Listing<Int|Float|String>|Int|Float|String)?
    
}