module org.schema.experimental
import "package://pkg.pkl-lang.org/pkl-pantry/pkl.experimental.uri@1.0.3#/URI.pkl" as URL
import "CreativeWorkSeries.pkl"
/// A video game series.
///
/// See: <https://schema.org/VideoGameSeries>
/// Model depth: 4
open class VideoGameSeries extends CreativeWorkSeries.CreativeWorkSeries {
        
    /// A season in a media series.
    seasons: (Listing<String>|String)?
        
    /// Indicate how many people can play this game (minimum, maximum, or range).
    numberOfPlayers: (Listing<String>|String)?
        
    /// An actor, e.g. in TV, radio, movie, video games etc. Actors can be associated with individual
    /// items or with a series, episode, clip.
    actors: (Listing<String>|String)?
        
    /// An episode of a TV, radio or game media within a series or season.
    episode: (Listing<String>|String)?
        
    /// The number of episodes in this season or series.
    numberOfEpisodes: (Listing<Int|String>|Int|String)?
        
    /// An actor, e.g. in TV, radio, movie, video games etc., or in an event. Actors can be associated
    /// with individual items or with a series, episode, clip.
    actor: (Listing<String>|String)?
        
    /// A director of e.g. TV, radio, movie, video gaming etc. content, or of an event. Directors
    /// can be associated with individual items or with a series, episode, clip.
    director: (Listing<String>|String)?
        
    /// The trailer of a movie or TV/radio series, season, episode, etc.
    trailer: (Listing<String>|String)?
        
    /// The composer of the soundtrack.
    musicBy: (Listing<String>|String)?
        
    /// Indicates whether this game is multi-player, co-op or single-player. The game can be
    /// marked as multi-player, co-op and single-player at the same time.
    playMode: (Listing<String>|String)?
        
    /// A season that is part of the media series.
    containsSeason: (Listing<String>|String)?
        
    /// The task that a player-controlled character, or group of characters may complete in
    /// order to gain a reward.
    quest: (Listing<String>|String)?
        
    /// An episode of a TV/radio series or season.
    episodes: (Listing<String>|String)?
        
    /// Real or fictional location of the game (or part of game).
    gameLocation: (Listing<URL|String>|URL|String)?
        
    /// A director of e.g. TV, radio, movie, video games etc. content. Directors can be associated
    /// with individual items or with a series, episode, clip.
    directors: (Listing<String>|String)?
        
    /// A season in a media series.
    season: (Listing<URL|String>|URL|String)?
        
    /// An item is an object within the game world that can be collected by a player or, occasionally,
    /// a non-player character.
    gameItem: (Listing<String>|String)?
        
    /// The number of seasons in this series.
    numberOfSeasons: (Listing<Int|String>|Int|String)?
        
    /// The electronic systems used to play <a href="http://en.wikipedia.org/wiki/Category:Video_game_platforms">video
    /// games</a>.
    gamePlatform: (Listing<URL|String>|URL|String)?
        
    /// The production company or studio responsible for the item, e.g. series, video game,
    /// episode etc.
    productionCompany: (Listing<String>|String)?
        
    /// Cheat codes to the game.
    cheatCode: (Listing<String>|String)?
        
    /// A piece of data that represents a particular aspect of a fictional character (skill,
    /// power, character points, advantage, disadvantage).
    characterAttribute: (Listing<String>|String)?
    
}