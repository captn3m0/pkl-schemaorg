module org.schema.experimental
import "package://pkg.pkl-lang.org/pkl-pantry/pkl.experimental.uri@1.0.3#/URI.pkl" as URL
import "StructuredValue.pkl"
/// A contact point&#x2014;for example, a Customer Complaints department.
///
/// See: <https://schema.org/ContactPoint>
/// Model depth: 4
open class ContactPoint extends StructuredValue.StructuredValue {
        
    /// Email address.
    email: (Listing<String>|String)?
        
    /// The hours during which this service or contact is available.
    hoursAvailable: (Listing<String>|String)?
        
    /// A person or organization can have different contact points, for different purposes.
    /// For example, a sales contact point, a PR contact point and so on. This property is used
    /// to specify the kind of contact point.
    contactType: (Listing<String>|String)?
        
    /// The telephone number.
    telephone: (Listing<String>|String)?
        
    /// The geographic area where the service is provided.
    serviceArea: (Listing<String>|String)?
        
    /// An option available on this contact point (e.g. a toll-free number or support for hearing-impaired
    /// callers).
    contactOption: (Listing<String>|String)?
        
    /// A language someone may use with or at the item, service or place. Please use one of the language
    /// codes from the [IETF BCP 47 standard](http://tools.ietf.org/html/bcp47). See also
    /// [[inLanguage]].
    availableLanguage: (Listing<String>|String)?
        
    /// The fax number.
    faxNumber: (Listing<String>|String)?
        
    /// The product or service this support contact point is related to (such as product support
    /// for a particular product line). This can be a specific product or product line (e.g. "iPhone")
    /// or a general category of products or services (e.g. "smartphones").
    productSupported: (Listing<String>|String)?
        
    /// The geographic area where a service or offered item is provided.
    areaServed: (Listing<String>|String)?
    
}