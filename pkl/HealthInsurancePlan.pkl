module org.schema.experimental
import "package://pkg.pkl-lang.org/pkl-pantry/pkl.experimental.uri@1.0.3#/URI.pkl" as URL
import "Intangible.pkl"
/// A US-style health insurance plan, including PPOs, EPOs, and HMOs.
///
/// See: <https://schema.org/HealthInsurancePlan>
/// Model depth: 3
open class HealthInsurancePlan extends Intangible.Intangible {
        
    /// TODO.
    healthPlanDrugOption: (Listing<String>|String)?
        
    /// The URL that goes directly to the plan brochure for the specific standard plan or plan
    /// variation.
    healthPlanMarketingUrl: (Listing<URL|String>|URL|String)?
        
    /// Networks covered by this plan.
    includesHealthPlanNetwork: (Listing<String>|String)?
        
    /// The URL that goes directly to the summary of benefits and coverage for the specific standard
    /// plan or plan variation.
    benefitsSummaryUrl: (Listing<URL|String>|URL|String)?
        
    /// The 14-character, HIOS-generated Plan ID number. (Plan IDs must be unique, even across
    /// different markets.)
    healthPlanId: (Listing<String>|String)?
        
    /// The standard for interpreting the Plan ID. The preferred is "HIOS". See the Centers for
    /// Medicare & Medicaid Services for more details.
    usesHealthPlanIdStandard: (Listing<URL|String>|URL|String)?
        
    /// The tier(s) of drugs offered by this formulary or insurance plan.
    healthPlanDrugTier: (Listing<String>|String)?
        
    /// Formularies covered by this plan.
    includesHealthPlanFormulary: (Listing<String>|String)?
        
    /// A contact point for a person or organization.
    contactPoint: (Listing<String>|String)?
    
}