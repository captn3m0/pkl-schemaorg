module org.schema.experimental
import "package://pkg.pkl-lang.org/pkl-pantry/pkl.experimental.uri@1.0.3#/URI.pkl" as URL
import "Reservation.pkl"
/// A reservation for lodging at a hotel, motel, inn, etc. Note: This type is for information
/// about actual reservations, e.g. in confirmation emails or HTML pages with individual
/// confirmations of reservations.
///
/// See: <https://schema.org/LodgingReservation>
/// Model depth: 4
open class LodgingReservation extends Reservation.Reservation {
        
    /// The number of children staying in the unit.
    numChildren: (Listing<Int|String>|Int|String)?
        
    /// Textual description of the unit type (including suite vs. room, size of bed, etc.).
    lodgingUnitType: (Listing<String>|String)?
        
    /// The earliest someone may check into a lodging establishment.
    checkinTime: (Listing<String|String>|String|String)?
        
    /// The number of adults staying in the unit.
    numAdults: (Listing<Int|String>|Int|String)?
        
    /// The latest someone may check out of a lodging establishment.
    checkoutTime: (Listing<String|String>|String|String)?
        
    /// A full description of the lodging unit.
    lodgingUnitDescription: (Listing<String>|String)?
    
}