module org.schema.experimental
import "package://pkg.pkl-lang.org/pkl-pantry/pkl.experimental.uri@1.0.3#/URI.pkl" as URL
import "CreativeWork.pkl"
/// A music recording (track), usually a single song.
///
/// See: <https://schema.org/MusicRecording>
/// Model depth: 3
open class MusicRecording extends CreativeWork.CreativeWork {
        
    /// The composition this track is a recording of.
    recordingOf: (Listing<String>|String)?
        
    /// The album to which this recording belongs.
    inAlbum: (Listing<String>|String)?
        
    /// The duration of the item (movie, audio recording, event, etc.) in [ISO 8601 date format](http://en.wikipedia.org/wiki/ISO_8601).
    duration: (Listing<String>|String)?
        
    /// The artist that performed this album or recording.
    byArtist: (Listing<String>|String)?
        
    /// The International Standard Recording Code for the recording.
    isrcCode: (Listing<String>|String)?
        
    /// The playlist to which this recording belongs.
    inPlaylist: (Listing<String>|String)?
    
}