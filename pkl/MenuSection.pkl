module org.schema.experimental
import "package://pkg.pkl-lang.org/pkl-pantry/pkl.experimental.uri@1.0.3#/URI.pkl" as URL
import "CreativeWork.pkl"
/// A sub-grouping of food or drink items in a menu. E.g. courses (such as 'Dinner', 'Breakfast',
/// etc.), specific type of dishes (such as 'Meat', 'Vegan', 'Drinks', etc.), or some other
/// classification made by the menu provider.
///
/// See: <https://schema.org/MenuSection>
/// Model depth: 3
open class MenuSection extends CreativeWork.CreativeWork {
        
    /// A subgrouping of the menu (by dishes, course, serving time period, etc.).
    hasMenuSection: (Listing<MenuSection|String>|MenuSection|String)?
        
    /// A food or drink item contained in a menu or menu section.
    hasMenuItem: (Listing<String>|String)?
    
}