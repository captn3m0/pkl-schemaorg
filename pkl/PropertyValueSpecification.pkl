module org.schema.experimental
import "package://pkg.pkl-lang.org/pkl-pantry/pkl.experimental.uri@1.0.3#/URI.pkl" as URL
import "Intangible.pkl"
/// A Property value specification.
///
/// See: <https://schema.org/PropertyValueSpecification>
/// Model depth: 3
open class PropertyValueSpecification extends Intangible.Intangible {
        
    /// The stepValue attribute indicates the granularity that is expected (and required)
    /// of the value in a PropertyValueSpecification.
    stepValue: (Listing<Int|Float|String>|Int|Float|String)?
        
    /// Indicates the name of the PropertyValueSpecification to be used in URL templates and
    /// form encoding in a manner analogous to HTML's input@name.
    valueName: (Listing<String>|String)?
        
    /// Specifies a regular expression for testing literal values according to the HTML spec.
    valuePattern: (Listing<String>|String)?
        
    /// Whether multiple values are allowed for the property. Default is false.
    multipleValues: (Listing<Boolean|String>|Boolean|String)?
        
    /// Whether or not a property is mutable. Default is false. Specifying this for a property
    /// that also has a value makes it act similar to a "hidden" input in an HTML form.
    readonlyValue: (Listing<Boolean|String>|Boolean|String)?
        
    /// The upper value of some characteristic or property.
    maxValue: (Listing<Int|Float|String>|Int|Float|String)?
        
    /// Specifies the allowed range for number of characters in a literal value.
    valueMaxLength: (Listing<Int|Float|String>|Int|Float|String)?
        
    /// The lower value of some characteristic or property.
    minValue: (Listing<Int|Float|String>|Int|Float|String)?
        
    /// Whether the property must be filled in to complete the action. Default is false.
    valueRequired: (Listing<Boolean|String>|Boolean|String)?
        
    /// The default value of the input. For properties that expect a literal, the default is a
    /// literal value, for properties that expect an object, it's an ID reference to one of the
    /// current values.
    defaultValue: (Listing<String>|String)?
        
    /// Specifies the minimum allowed range for number of characters in a literal value.
    valueMinLength: (Listing<Int|Float|String>|Int|Float|String)?
    
}