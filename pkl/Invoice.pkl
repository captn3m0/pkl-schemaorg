module org.schema.experimental
import "package://pkg.pkl-lang.org/pkl-pantry/pkl.experimental.uri@1.0.3#/URI.pkl" as URL
import "Intangible.pkl"
/// A statement of the money due for goods or services; a bill.
///
/// See: <https://schema.org/Invoice>
/// Model depth: 3
open class Invoice extends Intangible.Intangible {
        
    /// A number that confirms the given order or payment has been received.
    confirmationNumber: (Listing<String>|String)?
        
    /// An identifier for the method of payment used (e.g. the last 4 digits of the credit card).
    paymentMethodId: (Listing<String>|String)?
        
    /// The status of payment; whether the invoice has been paid or not.
    paymentStatus: (Listing<String>|String)?
        
    /// The total amount due.
    totalPaymentDue: (Listing<String>|String)?
        
    /// The date that payment is due.
    paymentDue: (Listing<String>|String)?
        
    /// The minimum payment required at this time.
    minimumPaymentDue: (Listing<String>|String)?
        
    /// The name of the credit card or other method of payment for the order.
    paymentMethod: (Listing<String>|String)?
        
    /// An entity that arranges for an exchange between a buyer and a seller. In most cases a broker
    /// never acquires or releases ownership of a product or service involved in an exchange.
    /// If it is not clear whether an entity is a broker, seller, or buyer, the latter two terms
    /// are preferred.
    broker: (Listing<String>|String)?
        
    /// The Order(s) related to this Invoice. One or more Orders may be combined into a single
    /// Invoice.
    referencesOrder: (Listing<String>|String)?
        
    /// The service provider, service operator, or service performer; the goods producer.
    /// Another party (a seller) may offer those services or goods on behalf of the provider.
    /// A provider may also serve as the seller.
    provider: (Listing<String>|String)?
        
    /// The date the invoice is scheduled to be paid.
    scheduledPaymentDate: (Listing<String>|String)?
        
    /// A category for the item. Greater signs or slashes can be used to informally indicate a
    /// category hierarchy.
    category: (Listing<URL|String>|URL|String)?
        
    /// Party placing the order or paying the invoice.
    customer: (Listing<String>|String)?
        
    /// The date that payment is due.
    paymentDueDate: (Listing<String|String>|String|String)?
        
    /// The identifier for the account the payment will be applied to.
    accountId: (Listing<String>|String)?
        
    /// The time interval used to compute the invoice.
    billingPeriod: (Listing<String>|String)?
    
}